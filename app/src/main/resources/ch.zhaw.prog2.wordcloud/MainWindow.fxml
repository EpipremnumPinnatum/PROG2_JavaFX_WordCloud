<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.zhaw.prog2.wordcloud.MainWindowController">
   <children>
      <VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="lableTitel" alignment="TOP_CENTER" text="Dieser Text wurde noch nicht hinzugefügt" textAlignment="CENTER" />
            <TextField fx:id="textEingabe" alignment="CENTER" prefHeight="25.0" prefWidth="306.0" />
            <HBox alignment="CENTER" prefHeight="190.0" prefWidth="640.0" spacing="10.0">
               <children>
                  <Button fx:id="leerenTextEingabe" alignment="TOP_LEFT" mnemonicParsing="false" text="Löschen Eingabefeld" />
                  <Button fx:id="hinzufuegenText" mnemonicParsing="false" text="Hinzufügen Text" />
               </children>
            </HBox>
            <TextArea fx:id="textHistory" maxWidth="615.0" prefHeight="235.0" prefWidth="615.0" VBox.vgrow="ALWAYS" />
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
